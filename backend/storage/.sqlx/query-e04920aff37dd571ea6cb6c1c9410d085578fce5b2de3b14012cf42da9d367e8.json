  "db_name": "PostgreSQL",
  "query": "\n                UPDATE users\n                SET\n                    permissions = (\n                        SELECT COALESCE(array_agg(p), ARRAY[]::user_permissions_enum[])\n                        FROM (\n                            SELECT unnest(permissions) AS p\n                            EXCEPT\n                            SELECT unnest($2::user_permissions_enum[])\n                            UNION\n                            SELECT unnest($3::user_permissions_enum[])\n                        ) AS combined\n                    ),\n                    max_snatches_per_day = $4\n                WHERE class_name = $1\n                RETURNING id\n            ",
